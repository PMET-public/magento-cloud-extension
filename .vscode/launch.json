  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${file}",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "outputCapture": "std",
      "name": "gulp",
      "console": "internalConsole",
      "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
      "args": [
        "dev"
      ]
    }
  ]
}